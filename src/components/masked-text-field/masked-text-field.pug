mixin defInput(options)
  div.text-field.js-text-field
    h6.text-field__label.js-text-field__label #{options.label}
    input.text-field__input.js-text-field__input(type="text",placeholder=options.placeholder)
    if options.arrow 
      img.filter-date-dropdown__arrow(src="assets/img/arrow_forward.jpg",alt="")


      
